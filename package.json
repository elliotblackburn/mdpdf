{
  "name": "mdpdf",
  "version": "1.3.4",
  "description": "Markdown to PDF command line converter",
  "main": "index.js",
  "scripts": {
    "test": "xo && mocha ./tests/index.js"
  },
  "bin": {
    "mdpdf": "bin/index.js"
  },
  "files": [
    "assets",
    "bin",
    "examples",
    "tests",
    "layout.hbs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluehatbrit/mdpdf.git"
  },
  "keywords": [
    "markdown",
    "md",
    "pdf",
    "converter"
  ],
  "author": {
    "name": "Elliot Blackburn",
    "email": "Elliot.blackburn@gmail.com",
    "url": "http://www.elliotblackburn.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bluehatbrit/mdpdf/issues"
  },
  "homepage": "https://github.com/bluehatbrit/mdpdf#readme",
  "dependencies": {
    "bluebird": "^3.4.7",
    "cheerio": "^0.22.0",
    "file-url": "^2.0.2",
    "handlebars": "^4.0.6",
    "html-pdf": "^2.1.0",
    "meow": "^3.7.0",
    "showdown": "^1.6.0",
    "showdown-emoji": "^1.0.3"
  },
  "devDependencies": {
    "execa": "^0.6.3",
    "mocha": "^3.2.0",
    "should": "^11.2.1",
    "xo": "^0.18.1"
  },
  "xo": {
    "ignores": [
      "assets/**",
      "tests/**"
    ],
    "rules": {
      "no-use-extend-native/no-use-extend-native": 0
    }
  }
